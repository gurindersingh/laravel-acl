{
  "name": "gurinder/laravel-acl",
  "description": "Laravel Roles And Permissions with UI",
  "keywords": [
    "laravel",
    "permission",
    "acl",
    "security"
  ],
  "homepage": "https://github.com/gurindersingh/laravel-aps",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gurinder Chauhan",
      "email": "gurinderchauhan@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/auth": "^5.6",
    "illuminate/container": "^5.6",
    "illuminate/contracts": "^5.6",
    "illuminate/database": "^5.6",
    "illuminate/events": "^5.6",
    "illuminate/pagination": "^5.6",
    "illuminate/routing": "^5.6",
    "illuminate/validation": "^5.6",
    "illuminate/view": "^5.6",
    "illuminate/cache": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Gurinder\\LaravelAcl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gurinder\\LaravelAcl\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gurinder\\LaravelAcl\\AclServiceProvider"
      ]
    }
  }
}
